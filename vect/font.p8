 dx=2,
 dy=2,
 size=function(_,x,y)
  _.dx=x
  _.dy=y or x
 end,
 find=function(s,c)
  local i,r=#s
  while i>0 do
   if sub(s,i,i)==c then
    r=i
    i=1
   end
   i-=1
  end
  return r
 end,
 pointtoxy=function(_,x,y,p)
  p=p-1
  return x+(p%5)*_.dx,y+(flr(p/5))*_.dy
 end,
 drawchar=function(_,x,y,char,c)
  local data,mx,p,x0,y0,x1,y1=_.char[char],0
  if data==nil and #char>1 then
   data=_.char[sub(char,2,2)]
  end
  if data~=nil then
   local i=1
   while i<=#data do
    p=font.find('abcdefghijklmnopqrstuvwxy',sub(data,i,i))
    if (p==nil) then
     i+=1
     x0,y0=_:pointtoxy(x,y,font.find('abcdefghijklmnopqrstuvwxy',sub(data,i,i)))
     if x0>mx then mx=x0 end
    else
     x1,y1=_:pointtoxy(x,y,p)
     if x1>mx then mx=x1 end
     line(x0,y0,x1,y1,c)
     x0=x1
     y0=y1
    end
    i+=1
   end
  end
  return mx
 end,
 drawtext=function(_,x,y,text,c)
  local prfx,char=''
  for i=1,#text do
   char=sub(text,i,i)
   if _.char[char]==nil then
    prfx=sub(text,i,i)
   else
    x=_:drawchar(x,y,prfx..sub(text,i,i),c)+_.dx
   end
  end
 end
}

