function pack(...)
 return {...}
end
function unpack(t, i)
  i = i or 1
  if i > #t then
   return
  else
	  return t[i], unpack(t, i+1)
	 end
end

function copy(t)
 return { unpack(t) }
end

a={1,2,3}
b=a
c=copy(a)
b[2]=4
print(a[2]..b[2]..c[2])
