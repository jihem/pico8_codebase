local c='++++++++++[>+++++++>++++++++++>+++>+<<<<-]>++.>+.+++++++..+++.>++.<<+++++++++++++++.>.+++.------.--------.>+.>.'
cls()
local a,m,s,i,e,l,j=1,{},{},1
while i<=#c do
 e=sub(c,i,i)
 if(e=='>') a+=1
 if(e=='<') a-=1
 if(e=='+') m[a]=(m[a] or 0)+1
 if(e=='-') m[a]=(m[a] or 0)-1  
 if(e=='.') print(m[a])
 if(e==']') then
  i=s[#s]-1
  del(s,i)
 end
 if(e=='[') then
  if m[a]>0 then
   s[#s]=i 
  else
   l=1
   j=i
   while j<#c and l>0 do
    j+=1
    e=sub(c,j,j)
    if (e=='[') l+=1
    if (e==']') l-=1
   end
   i=j
  end
 end
 i+=1
end
