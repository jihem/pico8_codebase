class={
 static_property='value',
 new=function(self,o)
  o=o or {}
  o.property=o.property or 'default'
  self.__index=self
  return setmetatable(o,self)
 end,
 method=function(self,argument)
  print(self.property..argument)
 end,
 static_method=function(argument)
  print(class.static_property..argument) 
 end
}

object=class:new({property='test'})
object:method('a')
object.static_method('a')
