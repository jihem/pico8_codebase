function unpack(t, i)
  i = i or 1
  if i > #t then
   return
  else
	  return t[i], unpack(t, i+1)
	 end
end

_exec=(function()
 local a={}
 return function(c)
  if type(c)=='function' then
   c(unpack(a))
   a={}
  else
   a[#a+1]=c
  end
 end
end)()
  
foreach({ cls, 63, 63, pset, 63, 63, 3, circ },_exec)
