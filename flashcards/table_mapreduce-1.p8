function _map(t,f)
 for k,v in pairs(t) do
  t[k]=f(k,v)
 end
end
function _reduce(t,f,a)
 for v in all(t) do
  a=f(v,a)
 end
 return a
end

t={1,3,5}
_map(t,function(k,v) 
 return k%2*v^2
end)
foreach(t,print)
print(_reduce(t,function(v,a)
 return v+a
end,0))
